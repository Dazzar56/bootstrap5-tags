{
  "name": "bootstrap5-tags",
  "version": "1.4.0",
  "description": "Replace select[multiple] with nices badges",
  "main": "tags",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "esbuild --bundle --minify --format=esm --sourcemap tags.js --outfile=tags.min.js",
    "start": "npm run build -- --servedir=.",
    "watch": "npm run build -- --watch",
    "build-standalone": "esbuild --bundle --minify --format=esm --sourcemap tags-standalone.js --outfile=tags-standalone.min.js",
    "start-standalone": "npm run build-standalone -- --servedir=.",
    "watch-standalone": "npm run build-standalone -- --watch",
    "build-min": "npm run build && npm run build-standalone && git add -A && git commit -m \"build min file\"",
    "bump": "npm run build-min && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lekoala/bootstrap5-tags"
  },
  "keywords": [
    "DOM",
    "tags",
    "select",
    "bootstrap",
    "bootstrap5",
    "bootstrap4",
    "es6"
  ],
  "author": "LeKoala",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap.native": "^4.0.7"
  }
}
